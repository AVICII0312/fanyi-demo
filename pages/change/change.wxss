.container{
  background:#f8f8f8;
  display: flex;
  flex-direction: column;
}
.title{
  background: #fff;
  border-bottom:1px solid #ececec;
  padding: 20rpx 40rpx;
  color: #aaa;
  font-size: 28rpx;
}
.item{
  padding-left: 40rpx;
  background:#fff;
}
.item-inner{
  display: flex;
  align-items: center;
  border-top: 1px solid #ececec;
  padding: 20rpx 40rpx 20rpx 0;
}
.item:nth-child(2) .item-inner{
  border-top: none;
}
.icon-duihao{
  font-size:30rpx;
  color: #aaa;
  margin-left: auto;
}