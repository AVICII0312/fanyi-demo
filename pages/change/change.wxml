<!--change.wxml-->
<view class="container lang-list">
  <view class="title">翻译成</view>
  <view class="item" hover-class="view-hover" data-chs="{{language.chs}}" data-index="{{index}}" data-lang="{{language.lang}}" wx:for="{{langlist}}" wx:for-item="language" wx:key="index" bindtap="onTabitem">
    <view class="item-inner">
      <text class="txt">{{language.chs}}</text>
      <text class="iconfont icon-duihao" wx:if="{{index===curlang.index}}"></text>
    </view>
  </view>
</view>